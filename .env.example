# Configurações do Sistema
ENV=development
LOG_LEVEL=INFO

# Rate Limiting (requisições por minuto por domínio)
RATE_LIMIT_DEFAULT=10
RATE_LIMIT_CARREFOUR=8
RATE_LIMIT_ATACADAO=10
RATE_LIMIT_PAO_ACUCAR=8
RATE_LIMIT_EXTRA=8

# Timeouts (segundos)
REQUEST_TIMEOUT=30
PLAYWRIGHT_TIMEOUT=60000

# Retries
MAX_RETRIES=3
RETRY_DELAY=5

# Paths
DATA_PATH=./data
LOG_PATH=./logs

# User Agent
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36